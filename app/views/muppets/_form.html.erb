<%= form_for muppet do |f| %>

<%= f.label "Muppet Name" %>
<%= f.text_field :name%>
<%= f.label "Show" %>
<%= f.fields_for :muppet_shows do |show| %>
    <%= show.collection_select :show_id, shows, :id, :name %>
    <%= show.label "Job" %>
    <%= show.text_field :job %>

    <% end %>
<%= f.submit "Hire Muppet"%>
<% end %>